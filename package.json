    "@types/d3": "^7.4.0",
    "@types/lodash": "^4.14.195",
    "@vueform/multiselect": "^2.6.2",
    "babel-runtime": "^6.26.0",
    "color-contrast": "^1.0.0",
    "d3": "^7.8.5",
    "d3-geo-projection": "^4.0.0",
    "date-fns": "^2.30.0",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "pixi.js": "^7.2.4",
    "vue": "^3.3.2",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
